{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "lib": ["esnext"],
    "strict": true,
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "noEmitOnError": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "types": [],
    "jsx": "react-jsx",
    "noEmit": false,
    "downlevelIteration": true
  },
  "exclude": ["node_modules", "dist"]
}
